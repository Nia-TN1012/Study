<?xml version="1.0" encoding="utf-8" ?>
<formats>
	<!-- 日報のタイトル -->
	<head>
		<title>
			<![CDATA[日報：{0} {1:MM/dd} {2}]]>
		</title>
	</head>
	<!-- 日報の本文 -->
	<body>
		<itemname>
			<![CDATA[◆ {0}]]>
		</itemname>
		<itemnamewithcount>
			<![CDATA[◆ {0}　{1}件]]>
		</itemnamewithcount>
		<description>
			<![CDATA[　{0}]]>
		</description>
		<descriptionforattendance>
